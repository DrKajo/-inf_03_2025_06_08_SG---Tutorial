KWERENDA 1 - SELECT Rodzaj, Nazwa, Gramatura, Cena FROM `wyroby` WHERE wyroby.Rodzaj = 'INNE';
KWERENDA 2 - SELECT DISTINCT Rodzaj FROM `wyroby` ORDER BY Rodzaj;
KWERENDA 3 - SELECT ID, Nazwa FROM `wyroby` WHERE Nazwa LIKE "%Chałka%";
KWERENDA 4 - SELECT DISTINCT Rodzaj, ROUND(AVG(Cena)) AS 'Średnia cena' FROM `wyroby` GROUP BY Rodzaj;

        $kw1 = mysqli_query("SELECT Rodzaj, Nazwa, Gramatura, Cena FROM `wyroby` WHERE wyroby.Rodzaj = '';");
        $kw2 = mysqli_query("SELECT DISTINCT Rodzaj FROM `wyroby` ORDER BY Rodzaj;");
        $kw3 = mysqli_query("SELECT ID, Nazwa FROM `wyroby` WHERE Nazwa LIKE "%Chałka%";");
        $kw4 = mysqli_query("SELECT DISTINCT Rodzaj, ROUND(AVG(Cena)) AS 'Średnia cena' FROM `wyroby` GROUP BY rodzaj;");